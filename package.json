{
  "name": "cg-test",
  "version": "1.0.0",
  "description": "Test repository for codeowners-git e2e testing",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "react-scripts start",
    "dev:backend": "nodemon backend/index.ts",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "react-scripts build",
    "build:backend": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write ."
  },
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/cors": "^2.8.13",
    "typescript": "^5.0.0",
    "react-scripts": "^5.0.1",
    "nodemon": "^2.0.22",
    "jest": "^29.5.0",
    "eslint": "^8.42.0",
    "prettier": "^2.8.8",
    "concurrently": "^8.0.1"
  },
  "keywords": [
    "test",
    "codeowners",
    "e2e",
    "testing"
  ],
  "author": "hemandev",
  "license": "MIT"
}